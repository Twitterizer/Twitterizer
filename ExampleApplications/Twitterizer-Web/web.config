<?xml version="1.0"?>
<configuration>
  <system.web>
    <customErrors mode="Off"/>
    <trace enabled="true" pageOutput="false" mostRecent="true" requestLimit="100"/>
    <compilation debug="true" targetFramework="4.0"/>
    <authentication mode="None"/>
    <pages masterPageFile="~/MasterPage.master" theme="DefaultTheme" controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
  <!-- The following block allows for diagnostic tracing information to be displayed in ASP.NET's page trace -->
  <system.diagnostics>
    <trace>
      <listeners>
        <add name="WebPageTraceListener" type="System.Web.WebPageTraceListener, System.Web, Version=2.0.3600.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>